<http://opendata.euskadi.eus/contenidos/ds_informes_estudios/calidad_aire_2017/es_def/adjuntos/datos_diarios.rdf>
  a dcat:Distribution ;
  dc:format [
    a dc:IMT ;
    rdfs:label "RDF-XML" ;
    rdf:value "text/rdf-xml"
  ] ;
  dcat:byteSize 0.0 ;
  dcat:accessURL "http://opendata.euskadi.eus/contenidos/ds_informes_estudios/calidad_aire_2017/es_def/adjuntos/datos_diarios.rdf"^^xsd:anyURI ;
  dc:title "Calidad del aire"@es .


